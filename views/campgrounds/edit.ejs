<% include ../partials/header %>
  <h1>Edit This Campground</h1>
  <form action='/campgrounds/<%=campground._id%>?_method=PUT', method='POST'>
      <input type='text' name='campground[title]' value="<%= campground.title %>">
      <input type='text' name='campground[image]' value="<%= campground.image %>">
      <input type='text' name='campground[description]' value="<%= campground.description %>">
      <button>Submit</button>
  </form>


<% include ../partials/footer %>